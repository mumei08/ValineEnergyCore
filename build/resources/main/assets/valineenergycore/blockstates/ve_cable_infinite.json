      "apply": { "model": "valineenergycore:block/cable/ve_cable_core" }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "valineenergycore:block/cable/ve_cable_north" }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "valineenergycore:block/cable/ve_cable_south" }
